{
  "tests": [
    {
      "name": "Create booking successfully",
      "method": "POST",
      "path": "/",
      "body": {
        "tourTitle": "Турция, Анталья",
        "hotel": "Rixos Premium Belek 5*",
        "duration": "7 ночей",
        "dates": "15-22 октября 2024",
        "price": 42000,
        "clientName": "Иван Иванов",
        "clientEmail": "ivan@example.com",
        "clientPhone": "+7 999 123-45-67",
        "adults": 2,
        "children": 1,
        "comment": "Хотим номер с видом на море"
      },
      "expectedStatus": 201,
      "expectedBody": {
        "success": true,
        "bookingId": "number",
        "createdAt": "string",
        "message": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Reject booking with missing fields",
      "method": "POST",
      "path": "/",
      "body": {
        "tourTitle": "Турция, Анталья",
        "clientName": "Иван Иванов"
      },
      "expectedStatus": 400,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}